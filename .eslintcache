ation/json' }));\r\n\r\n        if (!sendBeaconResult) {\r\n            var xhr = new XMLHttpRequest();\r\n            xhr.open('POST', this.counterUrl);\r\n            xhr.send(data);\r\n        }\r\n    }\r\n}\r\n\r\nexport default Counter",{"ruleId":"99","severity":1,"message":"100","line":49,"column":6,"nodeType":"101","endLine":49,"endColumn":8,"suggestions":"102"},{"ruleId":"103","replacedBy":"104"},{"ruleId":"105","replacedBy":"106"},{"ruleId":"107","severity":1,"message":"108","line":8,"column":5,"nodeType":"109","messageId":"110","endLine":8,"endColumn":15},{"ruleId":"99","severity":1,"message":"111","line":67,"column":8,"nodeType":"101","endLine":67,"endColumn":10,"suggestions":"112"},{"ruleId":"99","severity":1,"message":"113","line":62,"column":8,"nodeType":"101","endLine":62,"endColumn":27,"suggestions":"114"},{"ruleId":"103","replacedBy":"115"},{"ruleId":"105","replacedBy":"116"},{"ruleId":"107","severity":1,"message":"117","line":17,"column":7,"nodeType":"109","messageId":"110","endLine":17,"endColumn":14},{"ruleId":"107","severity":1,"message":"118","line":18,"column":7,"nodeType":"109","messageId":"110","endLine":18,"endColumn":16},"react-hooks/exhaustive-deps","React Hook useEffect has missing dependencies: 'catchPositionError' and 'catchWeatherAPIError'. Either include them or remove the dependency array.","ArrayExpression",["119"],"no-native-reassign",["120"],"no-negated-in-lhs",["121"],"@typescript-eslint/no-unused-vars","'Typography' is defined but never used.","Identifier","unusedVar","React Hook useEffect has missing dependencies: 'closeNavbar' and 'endAnimation'. Either include them or remove the dependency array.",["122"],"React Hook useEffect has missing dependencies: 'background.default' and 'text.primary'. Either include them or remove the dependency array.",["123"],["120"],["121"],"'API_KEY' is assigned a value but never used.","'PAGE_NAME' is assigned a value but never used.",{"desc":"124","fix":"125"},"no-global-assign","no-unsafe-neg